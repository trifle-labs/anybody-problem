{
  "name": "anybody-problem",
  "version": "1.0.0",
  "license": "CC-BY-NC-4.0",
  "main": "src/index.js",

  "scripts": {
    "build": "npx webpack --mode development",
    "chain": "hardhat node",
    "circom": "./scripts/basic_circom.sh",
    "circom:dev": "hardhat circom --deterministic --debug --verbose",
    "circom:prod": "hardhat circom --verbose",
    "dev": "npx webpack serve --mode development",
    "postinstall": "npx webpack --mode development",
    "sol-test": "hardhat test test/problems.test.js test/bodies.test.js test/solver.test.js",
    "test": "hardhat test"
  },
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "eslintConfig": {
    "env": {
      "amd": true,
      "browser": true,
      "es2020": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "modules": true
      },
      "ecmaVersion": 12,
      "parser": "@babel/eslint-parser",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    },
    "globals": {
      "assert": "readonly",
      "before": "readonly",
      "describe": "readonly",
      "it": "readonly"
    },
    "ignorePatterns": [
      "/docs/index.js"
    ],
    "root": true
  },
  "dependencies": {
    "prando": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.23.9",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomiclabs/hardhat-ethers": "*",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-ganache": "*",
    "@nomiclabs/hardhat-waffle": "*",
    "@openzeppelin/contracts": "^4.6.0",
    "babel-loader": "^9.1.3",
    "base64-sol": "^1.1.0",
    "chai": "*",
    "circomlib": "^2.0.3",
    "circomlibjs": "^0.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "ethereum-waffle": "*",
    "ethers": "^5.7.0",
    "hardhat": "2.13.0",
    "hardhat-circom": "git+ssh://git@github.com/okwme/hardhat-circom.git#514bf05c8aa1b5864484eb1e257c0df6053553f2",
    "hardhat-contract-sizer": "*",
    "hardhat-gas-reporter": "*",
    "html-webpack-plugin": "^5.5.0",
    "p5": "^1.8.0",
    "solidity-coverage": "^0.8.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.13.1"
  },
  "engines": {
    "node": ">=16"
  },
  "readme": "ERROR: No README data found!",
  "_id": "anybody-problem@1.0.0"
}
